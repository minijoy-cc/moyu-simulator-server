{
    "version": "0.2.0",
    "configurations": [
        // 这里的 `name` 应该符合你的二进制文件的文件名 
        {
            "name": "moyu-simulator-server",
            "type": "lldb",
            "request": "launch",

            // 这里填写二进制文件的路径, 根据自己的项目标准填写文件名
            "program": "${workspaceFolder}/target/debug/{PROJECT-NAME}",

            // 如果你需要传递一些参数在这里加入
            // "args": [],

            // 是否在调试前构建项目，可以为false
            "preLaunchTask": "build",

            // 如果你的项目以任何方式需要环境变量, 在这里加入
            "env": {},

        }
    ]
}
